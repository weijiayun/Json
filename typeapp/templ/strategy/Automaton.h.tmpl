struct FeedcodeMaxQty()
{
    required sint32 MaxQty;
    required string Feedcode;
};
enum OrderType::type
{
    OTGFD = 0,
    SIMULATION = 1,
    IOC = 2,
    POTF = 3
};

struct IInstructionComponent(struct:Struct)
{
    required string End reference DateTime;
    required string Start reference DateTime;
};

struct IStrategy(Strategy:strategy)
{
    required string Name;
    required sint32 Id;
    required string Type equal IStrategy;
};



struct Automaton(Strategy:strategy)
{
    required vec<3> I;
    required OrderType::type OrderType;
    optional list<TradingRange> Ranges;
    required mat<3,3> RiskMatrix;
    required sint32 Dimension;
    required string Type equal Automaton;
    required list<FeedcodeMaxQty> MaxOrderQty;
    required string SplitType default ShyGirl;
    required list<uint32> Strategies reference strategy;
    required list<IInstructionComponent> Components;
    optional string Description default "";
    required string Name;
    required sint32 Id auto;
    required sint32 Market reference market;
};



